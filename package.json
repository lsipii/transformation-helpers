{
  "name": "transformation-helpers",
  "version": "1.0.4",
  "description": "Utilities library for transforming objects, arrays, strings etc..",
  "directories": {
    "test": "tests"
  },
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "lint": "prettier --write 'src/**/*.ts'",
    "clean": "rimraf dist/*",
    "build": "npm run clean && babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files --extensions '.ts'",
    "prepare": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsipii/transformation-helpers.git"
  },
  "keywords": [
    "typescript",
    "utils"
  ],
  "author": "Lassi Piironen <lsipii@kapsi.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lsipii/transformation-helpers/issues"
  },
  "homepage": "https://github.com/lsipii/transformation-helpers#readme",
  "dependencies": {
    "buffer": "^6.0.3",
    "date-fns": "^2.28.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.5.2",
    "babel-jest": "^27.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}